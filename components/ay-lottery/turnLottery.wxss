.animt {
    animation: turn 1.2s;
}

@keyframes turn {
    0% {
        transform: perspective(150px) rotateY(0deg);
    }

    50% {
        transform: perspective(150px) rotateY(0deg);
    }

    100% {
        transform: perspective(150px) rotateY(179.9deg);
    }
}
