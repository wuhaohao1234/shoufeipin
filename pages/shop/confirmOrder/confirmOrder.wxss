/* pages/shop/confirmOrder/confirmOrder.wxss */
page{
    background-color: #f5f5f5;
}
.goods-img{
    width: 160rpx;
    height: 160rpx;
}

.list-item{
    height: 100rpx;
    line-height: 100rpx;
    border-bottom: 1rpx solid #efefef;
}

.list-item:last-child{
