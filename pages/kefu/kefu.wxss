/* pages/kefu/kefu.wxss */
.nav {
  position: relative;
  width: 600rpx;
  height: 120rpx;
  background-color: #f5f5f5;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 10rpx;
}
.ul {
  display: flex;
  width: 100%;
}
.nav-item {
  flex: 1;
  height: 120rpx;
  position: relative;
  list-style: none;
  z-index: 2;
  display: flex;
  justify-content: center;
}