page {
  background-color: #EAFCFC;
}

.nav-box {
  position: fixed;
  width: 100%;
  left: 0;
  top: 0;
  background: transparent;
  z-index: 11111;
}

.ad_box {
  position: fixed;
  top: 0;
  left: 0;
  display: flex;
  justify-content: center;
  align-items: flex-start;
  width: 750rpx;
  height: 100vh;
  background: #fff;
  z-index: 1000;
}

.nav-img {
  padding-left: 20rpx;
  display: flex;
  align-items: center;
}

.ad_img {
  height: 60%;
}


page::after {
  position: absolute;
  width: 177rpx;
  height: 177rpx;
  background: linear-gradient(206deg, #49EAC1 0%, #E8FBFF 100%);
  opacity: 0.5574;
  filter: blur(13.600000381469727px);
  top: 5rpx;
  right: 100rpx;
  z-index: -1;
  content: '';
}

page::before {
  z-index: -1;
  content: '';
  position: absolute;
  height: 1200rpx;
  width: 100%;
  background: linear-gradient(187deg, #49EAC1 0%, #EAFCFC 100%);
  top: 0;
}

.filter-box {
  width: 572rpx;
  height: 66rpx;
  background: rgba(255, 255, 255, 0.3098);
  box-shadow: inset 5rpx 3rpx 7rpx 0rpx rgba(255, 255, 255, 0.3098);
  opacity: 1;
  border: 1rpx solid #FFFFFF;
  margin: 20rpx auto;
  border-radius: 50rpx;
  display: flex;
  overflow: hidden;
}

.filter-box .filter-item {
  position: relative;
  height: 66rpx;
  line-height: 66rpx;
  width: 25%;
  text-align: center;
  color: #242428;
  font-size: 26rpx;

  color: #396968;
}

.filter-box .filter-item.sel::after {
  content: '';
  position: absolute;
  height: 65rpx;
  width: 1rpx;
  right: 0;
  background-color: #fff;
}

.filter-box .filter-item.sel {
  background: linear-gradient(180deg, rgba(255, 255, 255, 0.4196) 0%, rgba(255, 255, 255, 0.4196) 100%);

  color: #242428;
}

.my-ranking {
  text-align: right;
  margin-right: 40rpx;
  margin-top: 40rpx;
}

