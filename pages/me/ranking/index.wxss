/* 排行榜 */
.list-box {
  position: relative;
  width: 679rpx;
  background: rgba(255, 255, 255, 0.9);
  opacity: 1;
  border: 1rpx solid #FFFFFF;
  margin: -17rpx auto 20rpx auto;
  border-radius: 10rpx;
  padding: 30rpx;
  z-index: 99;
}

.list-box .list-title {
  display: flex;
  justify-content: space-between;

}


.list-box .list .item {
  display: flex;
  align-items: center;
  margin: 15rpx 0;
}

.list-box .list .item .no {

  color: #99A7AD;
  font-size: 40rpx;
  margin-right:
    15rpx;
}

.list-box .list .item .user-avatar {
  width: 70rpx;
  height: 70rpx;
  border: 4rpx solid #009F6E;
  border-radius: 50%;
  margin-right: 15rpx;
}

.list-box .list .item .info {
  width:
    400rpx;
}

.list-box .list .item .info .user-name {

  color: #333330;
  font-size: 30rpx;
}

.list-box .list .item .info .user-id {

  font-size: 24rpx;
