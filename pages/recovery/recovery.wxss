/* pages/recovery/recovery.wxss */
page {
  padding-bottom: 200rpx;
}

.nav-box {
  position: fixed;
  width: 100%;
  left: 0;
  top: 0;
  z-index: 11111;
  background: #49EAC1;
}

.ad_box {
  position: fixed;
  top: 0;
  left: 0;
  display: flex;
  justify-content: center;
  align-items: flex-start;
  width: 750rpx;
  height: 100vh;
  background: #fff;
  z-index: 1000;
}

.nav-img {
