}

.checked-icon {
	animation: none !important;
	transition: none !important;
	font-size: 20rpx;
}

.cate-checked::before{
	animation: all 0s !important;
	transition: none !important;
	background-color: #4FD1BC;
}

.cate-unchecked::before{
	animation: all 0s !important;
	transition: none !important;
	background-color: white;
}

.checkedbox-wrapper::before {
	content: '';
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	z-index: 1;
	height: 100%;
	background-color: #4FD1BC;
}

.checkbox-wrapper {
	margin-left: 10rpx;
	transform: scale(0.7, 0.7);
}

.checked-right {
  position: relative;
  right: 10px;
