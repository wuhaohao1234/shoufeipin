/* 弹出所有小分类的窗口包装 */
.all-category-popup-wrapper {
	height: 100vh;
	width: 100%;
	backdrop-filter: blur(5px);
	
	position: fixed;
	
	left: 0;
	bottom: 0;
	
	z-index: 10000;
}

/* 弹出所有小分类的窗口包装 */
.category-popup-wrapper {
	animation: move1 .3s;
	width: 100%;
	background-color: white;
	
	position: fixed;
	
	left: 0;
	bottom: 0;
	
	border-radius: 30rpx 30rpx 0 0;
	
	box-shadow: 0 -25rpx 60rpx -10rpx #3EC9BB;
	
	z-index: 10000;
	
	display: flex;
	justify-content: flex-start;
	align-items: center;
	flex-flow: column;
}

@keyframes move1 {
  from {
    transform: translateY(100%);
  }

  t0 {
    transform: translateY(10%);
  }
}

